{
    "name": "macocci7/file-selector-prompt",
    "type": "library",
    "description": "An Additional File Selector Prompt for your Laravel/Prompts Applications.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Macocci7\\FileSelectorPrompt\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/prompts": "^0.1.23"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "pestphp/pest": "^2.3",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan-mockery": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
